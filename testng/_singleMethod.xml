        <parameter name="browser" value="CHROME" />
        <parameter name="headed" value="true" />
        <classes>
            <class name="org.selenium.pom.tests.NavigationTest">
                <methods>
                    <include name="navigateFromHomeToFeaturedProductPage" />
                </methods>
            </class>
        </classes>
    </test>
</suite>
