<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Local Suite" verbose="1" thread-count="2" parallel="tests">

    <test name="Chrome" thread-count="7" parallel="methods">
        <parameter name="browser" value="CHROME"/>
        <parameter name="headed" value="true" />
        <packages>
            <package name="org.selenium.pom.tests"/>
        </packages>
    </test>

    <test name="Firefox" thread-count="7" parallel="methods">
        <parameter name="browser" value="FIREFOX"/>
        <parameter name="headed" value="true" />
        <packages>
            <package name="org.selenium.pom.tests"/>
        </packages>
    </test>
</suite>
